<?xml version="1.0" encoding="utf-8"?>
<odoo>

<!-- Disable Default Header -->
<record id="website.template_header_default" model="ir.ui.view">
    <field name="active" eval="False"/>
</record>

<!-- Activiate Boxed Header -->
<record id="website.template_header_boxed" model="ir.ui.view">
    <field name="active" eval="True"/>
</record>

<!-- Disable Default Footer -->
<record id="website.footer_custom" model="ir.ui.view">
    <field name="active" eval="False"/>
</record>

<!-- Activiate Centered Footer -->
<record id="website.template_footer_centered" model="ir.ui.view">
    <field name="active" eval="True"/>
</record>

</odoo>